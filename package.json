{
  "name": "configucius",
  "version": "0.4.0",
  "description": "config loader",
  "main": "dist",
  "scripts": {
    "build": "rimraf dist && babel src -d dist --source-maps",
    "lint": "eslint src tests",
    "test": "NODE_ENV=test mochista --opts tests/_/mocha.opts",
    "watch": "run-p \"build -- {@}\" \"test -- {@}\" -- \"--watch\"",
    "browser-sync": "browser-sync start --server coverage/lcov-report --files coverage/lcov-report/**/*.html",
    "index": "create-index src -ri --banner \"/* eslint-disable */\""
  },
  "repository": "laggingreflex/configucius",
  "author": "laggingreflex",
  "dependencies": {
    "arrify": "^1.0.1",
    "auto-bind": "^0.1.0",
    "deeper": "^2.1.0",
    "enquirer": "^0.4.1",
    "is-empty": "^1.0.0",
    "is-number-like": "^1.0.7",
    "json5": "^0.5.1",
    "lodash": "^4.17.2",
    "ority": "^0.3.3",
    "prompt-confirm": "^0.2.1",
    "prompt-password": "^0.1.1",
    "untildify": "^3.0.2",
    "yargs": "^5.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.16.0",
    "babel-plugin-annotate-console-log": "^1.0.0",
    "babel-plugin-rewire": "^1.0.0",
    "babel-plugin-transform-export-default-name": "^2.0.3",
    "babel-preset-es2015-node6": "^0.3.0",
    "babel-preset-stage-0": "^6.16.0",
    "create-index": "^1.0.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-like": "^0.1.10",
    "chokidar": "^1.6.1",
    "eslint": "^3.8.1",
    "mochista": "^0.3.0",
    "npm-run-all": "^3.1.1",
    "rimraf": "^2.5.4",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "standard": "^8.6.0"
  }
}
